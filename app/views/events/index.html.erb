<h1>Events</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Description</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Registration URL</th>
      <th>Registration Due</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.location %></td>
        <td><%= event.description %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= event.registration_url %></td>
        <td><%= event.registration_due %></td>
      </tr>
    <% end %>
  </tbody>
</table>
